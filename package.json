{
  "name": "@lab-anssi/ui-kit",
  "version": "1.30.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/lab-anssi-ui-kit.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/betagouv/lab-anssi-ui-kit/issues"
  },
  "homepage": "https://github.com/betagouv/lab-anssi-ui-kit#readme",
  "scripts": {
    "build": "vite build && npm run prepack",
    "build:webcomponents": "vite -c vite.webcomponents.config.ts build",
    "build:styles:dsfr": "sass src/lib/styles/dsfr-variables.scss dist/assets/dsfr-variables.css -I node_modules -I node_modules/@gouvfr/dsfr --no-source-map",
    "prepare": "husky",
    "prepack": "npm run build:webcomponents && npm run copy:icons && publint",
    "test": "vitest run",
    "storybook:dev": "STORYBOOK_ENV=development storybook dev -p 6006",
    "storybook:build": "STORYBOOK_ENV=production storybook build",
    "storybook:build-dev": "STORYBOOK_ENV=development storybook build",
    "storybook:test": "STORYBOOK_ENV=production npm run test-storybook",
    "test-storybook": "vitest --project=storybook",
    "copy:icons": "cp -r node_modules/@gouvfr/dsfr/dist/icons/* src/lib/assets/icones/dsfr && cp -r src/lib/assets/ dist/"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "type": "module",
  "peerDependencies": {
    "svelte": "^5.38.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.18.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@gouvfr/dsfr": "^1.14.1",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-svelte-csf": "^5.0.8",
    "@storybook/addon-themes": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/svelte-vite": "^9.1.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^22.18.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "cssnano": "^7.1.1",
    "dotenv": "^16.6.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^9.1.5",
    "eslint-plugin-svelte": "^3.12.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "postcss-discard-empty": "^7.0.1",
    "postcss-root-to-host": "^0.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "sass": "^1.92.1",
    "storybook": "^9.1.5",
    "svelte": "^5.38.8",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,ts,css,md,yml}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
